{
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": [
    "airbnb-base/legacy"
  ],
  "plugins": [
    "jquery"
  ],
  "globals": {
    "CL":true,
    "TweenMax":true,
    "TimelineLite" : false,
    "TimelineMax" : false,
    "TweenLite" : false,
    "TweenMax" : false,
    "Back" : false,
    "Bounce" : false,
    "Circ" : false,
    "Cubic" : false,
    "Ease" : false,
    "EaseLookup" : false,
    "Elastic" : false,
    "Expo" : false,
    "Linear" : false,
    "Power0" : false,
    "Power1" : false,
    "Power2" : false,
    "Power3" : false,
    "Power3" : false,
    "Power4" : false,
    "Quad" : false,
    "Quart" : false,
    "Quint" : false,
    "RoughEase" : false,
    "Sine" : false,
    "SlowMo" : false,
    "SteppedEase" : false,
    "Strong" : false,
    "Draggable" : false,
    "SplitText" : false,
    "VelocityTracker" : false,
    "CSSPlugin" : false,
    "ThrowPropsPlugin" : false,
    "BezierPlugin" : false
  },
  "rules": {
    "no-unused-vars": [2, {"vars": "local", "args": "after-used"}],
    "comma-dangle": 0,
    "no-console": 0,
    "no-underscore-dangle": 1,
    "quotes": [2, "single"],
    "func-names": 0
  }
}
